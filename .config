# Hostname and Port to connect to redis
REDIS_PORT=6379
REDIS_HOST=redis

# The host and container ports that will be mapped for the proxy server
PROXY_PORT=5000

# The ip address the proxy should listen on
PROXY_LISTEN=0.0.0.0

# The expiry time in seconds of a key in the cache
CACHE_EXPIRY=3

# The capacity of the cache (number of keys) after which the oldest key is evicted
CACHE_CAPACITY=2

# The max number of worker processes used by Gunicorn (and thus the number of parallel requests)
MAX_WORKER=3